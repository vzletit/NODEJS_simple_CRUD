  "bugs": {
    "url": "https://github.com/vzletit/NodeJS_simple_CRUD/issues"
  },
  "homepage": "https://github.com/vzletit/NodeJS_simple_CRUD#readme",
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@types/node": "^18.11.18",
    "@types/uuid": "^9.0.0",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "babel-jest": "^29.3.1",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^7.6.0",
    "eslint": "^8.31.0",
    "eslint-config-standard-with-typescript": "^26.0.0",
    "jest": "^29.3.1",
    "nodemon": "^2.0.20",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "dependencies": {
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "uuid": "^9.0.0"
  }
}
