{
  "name": "my-webpack-project",
  "type": "commonjs",
  "version": "1.0.0",
  "description": "My webpack project",
  "main": "./src/index.ts",
  "scripts": {
    "start:dev": "nodemon --watch src/ --exec \"npx tsc && node build/index\" --verbose -e ts",
    "start:dev:multi": "nodemon --watch src/ --exec \"npx tsc && node build/index --multi\" --verbose -e ts",
    "start:prod": "webpack && node dist/bundle",
    "start:multi": "webpack && node dist/bundle --multi",
    "build": "webpack --node-env=production",
    "test": "concurrently -k \"npx tsc && node build/index\" \"sleep 3 && cross-env NODE_OPTIONS=--experimental-vm-modules npx jest\""    
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vzletit/NodeJS_simple_CRUD.git"
  },
  "keywords": [],
